{
  "name": "DEX Sniper Pro",
  "short_name": "DEX Sniper",
  "description": "Professional DEX trading and sniping platform with automated strategies",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#0d6efd",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en-US",
  "categories": ["finance", "productivity", "business"],
  "screenshots": [
    {
      "src": "/images/screenshot-mobile.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Mobile trading interface"
    },
    {
      "src": "/images/screenshot-desktop.png", 
      "sizes": "1920x1080",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Desktop dashboard"
    }
  ],
  "icons": [
    {
      "src": "/images/icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/images/icon-512.png", 
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/images/icon-apple-touch.png",
      "sizes": "180x180", 
      "type": "image/png"
    }
  ],
  "shortcuts": [
    {
      "name": "Quick Trade",
      "short_name": "Trade",
      "description": "Execute manual trades",
      "url": "/?tab=trade",
      "icons": [
        {
          "src": "/images/shortcut-trade.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Autotrade",
      "short_name": "Auto",
      "description": "Automated trading dashboard", 
      "url": "/?tab=autotrade",
      "icons": [
        {
          "src": "/images/shortcut-autotrade.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Analytics",
      "short_name": "Analytics",
      "description": "Performance analytics",
      "url": "/?tab=analytics", 
      "icons": [
        {
          "src": "/images/shortcut-analytics.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Orders",
      "short_name": "Orders", 
      "description": "Advanced order management",
      "url": "/?tab=orders",
      "icons": [
        {
          "src": "/images/shortcut-orders.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+dexsniper",
      "url": "/?action=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/import",
      "accept": {
        "text/csv": [".csv"],
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": [".xlsx"]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": ["text/csv", "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"]
        }
      ]
    }
  },
  "prefer_related_applications": false,
  "related_applications": [],
  "iarc_rating_id": "",
  "dir": "ltr",
  "display_override": ["window-controls-overlay", "standalone", "browser"]
}